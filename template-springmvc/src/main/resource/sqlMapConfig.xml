<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <!--别名-->
  <typeAliases>
    <typeAlias alias="User" type="com.github.tanyueran.entity.User"/>
    <typeAlias alias="RedPacket" type="com.github.tanyueran.entity.RedPacket"/>
    <typeAlias alias="RedPacketDetail" type="com.github.tanyueran.entity.RedPacketDetail"/>
  </typeAliases>

  <!--mapper-->
  <mappers>
    <mapper resource="mapper/redPacket.xml"/>
    <mapper resource="mapper/redPacketDetail.xml"/>
    <mapper resource="mapper/userMapper.xml"/>
  </mappers>
</configuration>